{
  "version": "0.2.0",
  "configurations": [
      {
        "name": "apartments-api#attach",
        "type": "node",
        "request": "attach",
        "port": 5858,
        "address": "localhost",
        "restart": true,
        "sourceMaps": true,
        "outDir": null,
        "localRoot": "${workspaceRoot}/apartments-api",
        "remoteRoot": "."
    },
    {
      "name": "apartments-api#tests",
      "type": "node",
      "cwd": "${workspaceRoot}/apartments-api",
      "request": "launch",
      "program": "${workspaceRoot}/apartments-api/node_modules/mocha/bin/_mocha",
      "stopOnEntry": false,
      "args": [ "test/shared/before-tests.js", "test/**/*.spec.js" ],
      "preLaunchTask": null,
      "port": 5858,
      "runtimeExecutable": "${workspaceRoot}/apartments-api/node_modules/babel-cli/bin/babel-node.js",
      "runtimeArgs": [ "--nolazy" ],
      "env": { "NODE_ENV": "test" },
      "console": "internalConsole",
      "sourceMaps": true,
      "outDir": null
    },
    {
      "name": "front-gateway",
      "type": "node",
      "cwd": "${workspaceRoot}/front-gateway",
      "request": "launch",
      "program": "${workspaceRoot}/front-gateway/src/server/server.js",
      "stopOnEntry": false,
      "args": [ ],
      "preLaunchTask": null,
      "port": 5859,
      "runtimeExecutable": "${workspaceRoot}/front-gateway/scripts/debug.sh",
      "runtimeArgs": [ "--nolazy" ],
      "env": { "NODE_ENV": "development" },
      "console": "internalConsole",
      "sourceMaps": true,
      "outDir": null
    },
  ]
}
