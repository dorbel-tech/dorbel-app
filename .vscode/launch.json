{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "apartments-api#attach",
      "type": "node",
      "request": "attach",
      "port": 5858,
      "address": "localhost",
      "restart": true,
      "sourceMaps": true,
      "localRoot": "${workspaceRoot}/apartments-api",
      "remoteRoot": "."
    },
    {
      "name": "apartments-api#unit-tests",
      "type": "node",
      "cwd": "${workspaceRoot}/apartments-api",
      "request": "launch",
      "args": [ "run", "test:unit", "--", "--debug", "--debug-brk" ],
      "port": 5858,
      "runtimeExecutable": "npm",
      "runtimeArgs": [ ],
      "console": "internalConsole",
      "sourceMaps": true
    },
    {
      "name": "apartments-api#integration-tests",
      "type": "node",
      "cwd": "${workspaceRoot}/apartments-api",
      "request": "launch",
      "args": [ "run", "test:integration", "--", "--debug", "--debug-brk" ],
      "port": 5858,
      "runtimeExecutable": "npm",
      "runtimeArgs": [ ],
      "console": "internalConsole",
      "sourceMaps": true
    },
    {
      "name": "front-gateway",
      "type": "node",
      "cwd": "${workspaceRoot}/front-gateway",
      "request": "launch",
      "program": "${workspaceRoot}/front-gateway/src/server/server.js",
      "stopOnEntry": false,
      "args": [ ],
      "preLaunchTask": null,
      "port": 5859,
      "runtimeExecutable": "${workspaceRoot}/front-gateway/scripts/debug.sh",
      "runtimeArgs": [ "--nolazy" ],
      "env": { "NODE_ENV": "development" },
      "console": "internalConsole",
      "sourceMaps": true
    },
    {
      "name": "notifications-service#attach",
      "type": "node",
      "request": "attach",
      "port": 5861,
      "address": "localhost",
      "restart": true,
      "sourceMaps": true,
      "localRoot": "${workspaceRoot}/notifications-service",
      "remoteRoot": "."
    },
    {
      "name": "ohe-api#attach",
      "type": "node",
      "request": "attach",
      "port": 5860,
      "address": "localhost",
      "restart": true,
      "sourceMaps": true,
      "localRoot": "${workspaceRoot}/ohe-api",
      "remoteRoot": "."
    }
  ]
}
