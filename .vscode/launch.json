    {
      "name": "ohe-api#attach",
      "type": "node",
      "request": "attach",
      "port": 5860,
      "address": "localhost",
      "restart": true,
      "sourceMaps": true,
      "localRoot": "${workspaceRoot}/ohe-api",
      "remoteRoot": "/home/nodejs/app",
      "smartStep": true
    }
  ]
