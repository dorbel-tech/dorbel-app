{
  "/event": {
    "post": {
      "summary": "Create an open house event",
      "description": "Create an open house event to the system",
      "parameters": [
        {
          "name": "body",
          "in": "body",
          "schema": {
            "$ref": "#/definitions/AddOpenHouseEventRequest"
          }
        },
        {
          "name": "x-user-profile",
          "in": "header",
          "required": true,
          "type": "string",
          "description": "Stringified JSON object with the user's profile information"
        }
      ],
      "tags": [
        "openHouseEventsController",
        "authenticated"
      ],
      "responses": {
        "201": {
          "description": "Open house event created succesfully",
          "schema": {
            "$ref": "#/definitions/OpenHouseEvent"
          }
        },
        "default": {
          "description": "Unexpected error",
          "schema": {
            "$ref": "#/definitions/Error"
          }
        }
      }
    }
  },
  "/event/{id}": {
    "get": {
      "summary": "Finds an open house event",
      "description": "Finds an open house event in the system",
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "type":"integer",
          "required": true
        }
      ],
      "tags": [
        "openHouseEventsController"
      ],
      "responses": {
        "201": {
          "description": "Open house event found succesfully",
          "schema": {
            "$ref": "#/definitions/OpenHouseEvent"
          }
        },
        "default": {
          "description": "Unexpected error",
          "schema": {
            "$ref": "#/definitions/Error"
          }
        }
      }
    },
    "put": {
      "summary": "Update an open house event",
      "description": "Update an open house event to the system",
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "type":"integer",
          "required": true
        },
        {
          "name": "body",
          "in": "body",
          "schema": {
            "$ref": "#/definitions/UpdateOpenHouseEventRequest"
          }
        },
        {
          "name": "x-user-profile",
          "in": "header",
          "required": true,
          "type": "string",
          "description": "Stringified JSON object with the user's profile information"
        }
      ],
      "tags": [
        "openHouseEventsController",
        "authenticated"
      ],
      "responses": {
        "200": {
          "description": "Open house event updated succesfully",
          "schema": {
            "$ref": "#/definitions/OpenHouseEvent"
          }
        },
        "default": {
          "description": "Unexpected error",
          "schema": {
            "$ref": "#/definitions/Error"
          }
        }
      }
    },
    "delete": {
      "summary": "Deletes an open house event",
      "description": "Deletes an open house event to the system",
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "type":"integer",
          "required": true
        },
        {
          "name": "x-user-profile",
          "in": "header",
          "required": true,
          "type": "string",
          "description": "Stringified JSON object with the user's profile information"
        }
      ],
      "tags": [
        "openHouseEventsController",
        "authenticated"
      ],
      "responses": {
        "200": {
          "description": "Open house event deleted succesfully"
        },
        "default": {
          "description": "Unexpected error",
          "schema": {
            "$ref": "#/definitions/Error"
          }
        }
      }
    }
  },
  "/events/by-listing/{listingIds}": {
    "get": {
      "summary": "Finds all an open house events for a listing",
      "description": "Finds all an open house events for a listing in the system",
      "parameters": [
        {
          "name": "listingIds",
          "description": "A comma delimted list of IDs",
          "in": "path",
          "type": "string",
          "required": true
        },
        {
          "name": "minDate",
          "description": "Minimum start date of events",
          "in": "query",
          "type": "string",
          "format": "date"
        }
      ],
      "tags": [
        "listingEventsController"
      ],
      "responses": {
        "200": {
          "description": "Open house event for listing found succesfully",
          "schema": {
            "$ref": "#/definitions/OpenHouseEvents"
          }
        },
        "default": {
          "description": "Unexpected error",
          "schema": {
            "$ref": "#/definitions/Error"
          }
        }
      }
    }
  }
}
