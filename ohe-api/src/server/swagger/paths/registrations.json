{
  "/event/registration": {
    "post": {
      "summary": "Register user to an open house event",
      "description": "Create an open house event registration to the system",
      "parameters": [
        {
          "name": "body",
          "in": "body",
          "schema": {
            "$ref": "#/definitions/AddRegistrationRequest"
          }
        },
        {
          "name": "x-user-profile",
          "in": "header",
          "required": true,
          "type": "string",
          "description": "Stringified JSON object with the user's profile information"
        }
      ],
      "tags": [
        "registrationsController",
        "authenticated"
      ],
      "responses": {
        "201": {
          "description": "Registration created succesfully",
          "schema": {
            "$ref": "#/definitions/Registration"
          }
        },
        "default": {
          "description": "Unexpected error",
          "schema": {
            "$ref": "#/definitions/Error"
          }
        }
      }
    }
  },
  "/event/registration/{id}": {
    "delete": {
      "summary": "Unregister user from an open house event",
      "description": "Deletes a registration for an open house event to the system",
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "type":"integer",
          "required": true
        },
        {
          "name": "x-user-profile",
          "in": "header",
          "required": true,
          "type": "string",
          "description": "Stringified JSON object with the user's profile information"
        }
      ],
      "tags": [
        "registrationsController",
        "authenticated"
      ],
      "responses": {
        "200": {
          "description": "Registration deleted succesfully"
        },
        "default": {
          "description": "Unexpected error",
          "schema": {
            "$ref": "#/definitions/Error"
          }
        }
      }
    }
  }
}
