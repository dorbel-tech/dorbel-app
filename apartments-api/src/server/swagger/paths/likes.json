{
  "/likes/user": {
    "get": {
      "summary": "get all listingIds that were liked by the current user",
      "description": "Returns the IDs of all the listings that were liked by the current user",
      "parameters": [
        {
          "name": "x-user-profile",
          "in": "header",
          "required": true,
          "type": "string",
          "description": "Stringified JSON object with the user's profile information"
        }
      ],
      "tags": [
        "userLikeController",
        "authenticated"
      ],
      "responses": {
        "200": {
          "description": "operation was successful, return dictionary of listingId keys and bool value to indicate if liked",
          "schema": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          }
        },
        "default": {
          "description": "Unexpected error",
          "schema": {
            "$ref": "#/definitions/Error"
          }
        }
      }
    }
  },
  "/likes/{listingId}": {
    "get": {
      "summary": "Get likes by listing",
      "description": "Get active likes by listing id",
      "parameters": [
        {
          "name": "listingId",
          "in": "path",
          "type":"integer",
          "required": true
        }
      ],
      "tags": [
        "likeController"
      ],
      "responses": {
        "200": {
          "description": "Likes by listing",
          "schema": {
            "$ref": "#/definitions/Likes"
          }
        },
        "default": {
          "description": "Unexpected error",
          "schema": {
            "$ref": "#/definitions/Error"
          }
        }
      }
    },
    "post": {
      "summary": "Marks a listing as 'liked'",
      "description": "Marks a listing as 'liked'",
      "parameters": [
        {
          "name": "listingId",
          "in": "path",
          "description": "id of listing to like",
          "type": "string",
          "required": true
        },
        {
          "name": "x-user-profile",
          "in": "header",
          "required": true,
          "type": "string",
          "description": "Stringified JSON object with the user's profile information"
        }
      ],
      "tags": [
        "likeController",
        "authenticated"
      ],
      "responses": {
        "200": {
          "description": "operation was successful"
        },
        "default": {
          "description": "Unexpected error",
          "schema": {
            "$ref": "#/definitions/Error"
          }
        }
      }
    },
    "delete": {
      "summary": "Marks a listing as 'unliked'",
      "description": "Marks a listing as 'unliked'",
      "parameters": [
        {
          "name": "listingId",
          "in": "path",
          "description": "id of listing to unlike",
          "type": "string",
          "required": true
        },
        {
          "name": "x-user-profile",
          "in": "header",
          "required": true,
          "type": "string",
          "description": "Stringified JSON object with the user's profile information"
        }
      ],
      "tags": [
        "likeController",
        "authenticated"
      ],
      "responses": {
        "200": {
          "description": "operation was successful"
        },
        "default": {
          "description": "Unexpected error",
          "schema": {
            "$ref": "#/definitions/Error"
          }
        }
      }
    }
  }
}
