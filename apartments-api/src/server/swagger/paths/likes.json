      ],
      "tags": [
        "likeController",
        "authenticated"
      ],
      "responses": {
        "200": {
          "description": "operation was successful"
        },
        "default": {
          "description": "Unexpected error",
          "schema": {
            "$ref": "#/definitions/Error"
          }
        }
      }
    },
    "delete": {
      "summary": "Marks an apartment as unliked",
      "description": "Marks an apartment as unliked",
      "parameters": [
        {
