{
  "/sendMonthlyReport": {
    "post": {
      "description": "Returns and array of {listing_id, publishing_user_id} representing the listings for which the report will be sent for and publishes a SEND_MONTHLY_REPORT message for each one",
      "parameters": [
        {
          "name": "day",
          "in": "body",
          "description": "The day of 'lease_start'",
          "type": "integer",
          "required": true,
          "minimum": 1,
          "maximum": 31
        },
        {
          "name": "month",
          "in": "body",
          "description": "The month of 'lease_start'",
          "type": "integer",
          "required": true,
          "minimum": 1,
          "maximum": 12
        },
        {
          "name": "year",
          "in": "body",
          "description": "The month of 'lease_start'",
          "type": "integer",
          "required": true,
          "minimum": 1900
        }
      ],
      "tags": [
        "monthlyReportController",
        "authenticated"
      ],
      "responses": {
        "200": {
          "schema": {
            "type": "array"
          }
        },
        "default": {
          "description": "Unexpected error",
          "schema": {
            "$ref": "#/definitions/Error"
          }
        }
      }
    }
  }
}