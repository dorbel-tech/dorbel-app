{
  "/listings": {
    "get": {
      "summary": "List Listings",
      "description": "The Listing endpoint returns the available listings",
      "parameters": [
        {
          "name": "city_name",
          "in": "query",
          "description": "City listing are in",
          "type": "string"
        }
      ],
      "tags": [
        "listingController"
      ],
      "responses": {
        "200": {
          "description": "An array of apartments",
          "schema": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/Listing"
            }
          }
        },
        "default": {
          "description": "Unexpected error",
          "schema": {
            "$ref": "#/definitions/Error"
          }
        }
      }
    },
    "post": {
      "summary": "Post new listing",
      "description": "Add a new listing to the system",
      "parameters": [
        {
          "name": "body",
          "in": "body",
          "schema": {
            "$ref": "#/definitions/Listing"
          }
        },
        {
          "name": "x-user-profile",
          "in": "header",
          "required": true,
          "type": "string",
          "description": "Stringified JSON object with the user's profile information"
        }
      ],
      "tags": [
        "listingController"
      ],
      "responses": {
        "201": {
          "description": "Listing created succesfully",
          "schema": {
            "$ref": "#/definitions/Listing"
          }
        },
        "default": {
          "description": "Unexpected error",
          "schema": {
            "$ref": "#/definitions/Error"
          }
        }
      }
    }
  }
}
