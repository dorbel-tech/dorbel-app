{
  "name": "dorbel-apartment-api",
  "version": "0.0.1",
  "description": "API for dorbel apartments domain",
  "main": "src/index.js",
  "scripts": {
    "docker-build": "docker build -t dorbel-apt-api-image:latest .",
    "docker-run": "docker run -it -v $PWD/src:/home/nodejs/app/src -p 3000:3000 --name dorbel-apt-api dorbel-apt-api-image npm run-script start-dev; docker stop dorbel-apt-api",
    "docker-start": "docker start -a dorbel-apt-api; docker stop dorbel-apt-api",
    "docker-clear": "docker stop dorbel-apt-api; docker rm dorbel-apt-api; docker rmi dorbel-apt-api-image",
    "start": "babel-node src/index",
    "start-dev": "nodemon --exec babel-node src/index | bunyan",
    "test": "mocha --compilers js:babel-register test/shared/before-tests.js test/**/*.js"
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "dorbel-shared": "file:../dorbel-shared",
    "fleek-router": "^1.2.5",
    "koa": "^1.2.4",
    "koa-bodyparser": "^2.2.0",
    "lodash": "^4.16.3",
    "swagger-validate": "^0.1.7"
  },
  "devDependencies": {
    "babel-register": "^6.16.3",
    "co-mocha": "^1.1.3",
    "co-supertest": "0.0.10",
    "hamjest": "^2.13.0",
    "mocha": "^3.1.0",
    "nodemon": "^1.10.2",
    "supertest": "^1.2.0"
  }
}
