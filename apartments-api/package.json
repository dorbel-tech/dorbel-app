{
  "name": "dorbel-apartment-api",
  "version": "0.0.81",
  "description": "API for dorbel apartments domain",
  "main": "src/index.js",
  "scripts": {
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "prestart": "yarn run db:migrate; yarn run db:seed",
    "start": "babel-node src/index",
    "start:dev": "yarn run db:migrate; yarn run db:seed && nodemon --exec babel-node --debug=5858 src/index | bunyan",
    "test": "yarn run test:unit",
    "test:unit": "mocha --compilers js:babel-register test/shared/before-tests.js test/unit/**/*.spec.js",
    "test:integration": "mocha --compilers js:babel-register test/shared/before-tests.js test/integration/**/*.spec.js",
    "lint": "eslint --ext .js -f table src",
    "check": "satisfaction-status || yarn install"
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-register": "^6.16.3",
    "co": "^4.6.0",
    "co-mocha": "^1.1.3",
    "co-supertest": "0.0.10",
    "dorbel-shared": "git://github.com/dorbel-tech/dorbel-shared#v0.4.51",
    "eslint": "^3.7.1",
    "eslint-plugin-flowtype": "^2.19.0",
    "faker": "^3.1.0",
    "fleek-router": "^1.2.5",
    "hamjest": "^2.13.0",
    "koa": "^1.2.4",
    "koa-bodyparser": "^2.2.0",
    "lodash": "^4.16.3",
    "mocha": "^3.1.0",
    "mock-require": "^1.3.0",
    "moment": "~2.16.0",
    "mysql": "^2.11.1",
    "node-geocoder": "^3.15.1",
    "request-promise": "^4.1.1",
    "sequelize": "3.25.0",
    "sequelize-cli": "^2.5.1",
    "sinon": "^1.17.6",
    "sinon-as-promised": "^4.0.2",
    "sqlite3": "^3.1.8",
    "sqlstring": "^2.2.0",
    "supertest": "^1.2.0"
  },
  "devDependencies": {
    "bunyan": "^1.8.1",
    "nodemon": "^1.10.2",
    "satisfaction": "^3.0.0"
  }
}
