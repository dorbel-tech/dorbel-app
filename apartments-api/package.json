{
  "name": "apartments-api",
  "version": "1.1.16",
  "description": "API for dorbel apartments domain",
  "main": "src/index.js",
  "scripts": {
    "create:listings": "mocha --timeout 10000 --compilers js:babel-register test/shared/before-tests.js test/integration/apartments-api.spec.js",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:clean": "./scripts/clean-db.sh",
    "start": "yarn run db:migrate && yarn run db:seed && babel-node src/index",
    "start:dev": "yarn run check; yarn run db:migrate; yarn run db:seed && yarn run test:seed && nodemon --exec babel-node --inspect=5858 src/index | bunyan -o short",
    "test": "yarn run test:unit",
    "test:unit": "yarn run wait && nyc --all mocha --timeout 10000 --compilers js:babel-register test/shared/before-tests.js test/unit/**/*.spec.js",
    "test:integration": "./scripts/integration-tests.sh",
    "test:seed": "node src/apartmentsDb/testSeed/testSeed.js",
    "lint": "eslint --ext .js -f table src",
    "check": "satisfaction-status || yarn install",
    "wait": "./scripts/wait-for-it.sh apartments-api:3000 --timeout=180",
    "wait:db": "./scripts/wait-for-it.sh db:3306 --timeout=180"
  },
  "dependencies": {
    "apollo-server-koa": "^1.0.4",
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-register": "^6.16.3",
    "co": "^4.6.0",
    "co-mocha": "^1.1.3",
    "co-supertest": "0.0.10",
    "dorbel-shared": "git://github.com/dorbel-tech/dorbel-shared#v1.0.71",
    "dotenv": "^4.0.0",
    "eslint": "^3.7.1",
    "eslint-plugin-flowtype": "^2.19.0",
    "faker": "^3.1.0",
    "fleek-router": "^1.2.5",
    "googleapis": "^17.1.0",
    "graphql": "^0.10.5",
    "graphql-tools": "^1.1.0",
    "hamjest": "^2.13.0",
    "koa": "1.2.5",
    "koa-bodyparser": "^2.2.0",
    "lodash": "^4.16.3",
    "mocha": "^3.1.0",
    "mock-require": "^1.3.0",
    "moment": "~2.16.0",
    "mysql": "^2.11.1",
    "node-geocoder": "^3.15.1",
    "nyc": "^10.1.2",
    "sequelize": "^3.30.2",
    "sequelize-cli": "^2.5.1",
    "sinon": "^1.17.6",
    "sinon-as-promised": "^4.0.2",
    "sqlite3": "^3.1.8",
    "supertest": "^1.2.0"
  },
  "devDependencies": {
    "bunyan": "^1.8.1",
    "graphql-tag": "^2.4.2",
    "nodemon": "^1.10.2",
    "satisfaction": "^3.0.0"
  },
  "nyc": {
    "reporter": [
      "json"
    ],
    "include": [
      "src/**/*.{js}"
    ],
    "report-dir": "./test/coverage"
  }
}
