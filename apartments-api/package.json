{
  "name": "dorbel-apartment-api",
  "version": "0.0.50",
  "description": "API for dorbel apartments domain",
  "main": "src/index.js",
  "scripts": {
    "start": "babel-node src/index",
    "start:dev": "npm run check; nodemon --exec babel-node src/index | bunyan",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint --ext .js -f table src",
    "test:unit": "mocha --compilers js:babel-register test/shared/before-tests.js test/unit/**/*.spec.js",
    "test:integration": "mocha --compilers js:babel-register test/shared/before-tests.js test/integration/**/*.spec.js",
    "check": "satisfaction-status || npm install"
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "co": "^4.6.0",
    "dorbel-shared": "dorbel-tech/dorbel-shared#v0.4.3",
    "fleek-router": "^1.2.5",
    "koa": "^1.2.4",
    "koa-bodyparser": "^2.2.0",
    "lodash": "^4.16.3",
    "mysql": "^2.11.1",
    "sequelize": "^3.24.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-register": "^6.16.3",
    "bunyan": "^1.8.1",
    "co-mocha": "^1.1.3",
    "co-supertest": "0.0.10",
    "eslint": "^3.7.1",
    "eslint-plugin-flowtype": "^2.19.0",
    "flow-bin": "0.33.0",
    "hamjest": "^2.13.0",
    "mocha": "^3.1.0",
    "nodemon": "^1.10.2",
    "satisfaction": "3.0.0",
    "supertest": "^1.2.0"
  }
}
