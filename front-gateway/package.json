{
  "name": "front-gateway",
  "version": "1.0.9",
  "description": "Front end application for dorbel",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./public/build",
    "start": "babel-node src/server/server.js",
    "build": "yarn run clean; NODE_ENV=production webpack",
    "start:dev": "nodemon --exec babel-node --debug=5859 src/server/server.js | bunyan -o short",
    "build:dev": "NODE_ENV=development webpack-dev-server --content-base public -d --config webpack.config.js --host 0.0.0.0 --colors --inline --no-info",
    "test": "yarn run test:unit",
    "test:unit": "jest",
    "test:unit:codecov": "jest && codecov -t d5d2f1f7-150d-4990-a466-029830f6a8f8",
    "test:e2e": "nightwatch",
    "test:e2e:browserstack": "RUN_ON_BSTACK=true nightwatch -c test/browserstack.conf.js",
    "test:e2e:browserstack:local": "BSTACK_LOCAL=true yarn run test:e2e:browserstack",
    "test:integration": "mocha --timeout 10000 --compilers js:babel-register test/integration/before-tests.js test/integration/**/*.js",
    "lint": "stylelint 'src/components/**/*.scss' && eslint --ext .jsx,.js -f table src",
    "check": "satisfaction-status || yarn install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "auth0": "^2.5.0",
    "auth0-lock": "^10.4.0",
    "axios": "0.15.2",
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.0.0",
    "babel-jest": "18.0.0",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-root-import": "^4.1.3",
    "babel-traverse": "6.19.0",
    "bootstrap-sass-directional": "2.1.1",
    "browserstack-automate": "^1.0.1",
    "chromedriver": "^2.25.3",
    "co": "^4.6.0",
    "codecov": "^1.0.1",
    "cookies.js": "madmurphy/cookies.js",
    "css-loader": "^0.26.1",
    "director": "^1.2.8",
    "dorbel-shared": "git://github.com/dorbel-tech/dorbel-shared#v1.0.16",
    "ejs": "^2.5.2",
    "enzyme": "2.7.1",
    "es6-promisify": "^5.0.0",
    "eslint": "^3.7.1",
    "eslint-plugin-flowtype": "^2.19.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "beta",
    "file-loader": "0.9.0",
    "formsy-react": "^0.19.2",
    "formsy-react-components": "^0.10.1",
    "google-map-react": "^0.22.3",
    "ignore-styles": "^5.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "18.1.0",
    "jwt-decode": "2.1.0",
    "koa": "^1.2.4",
    "koa-compress": "^1.0.9",
    "koa-ejs": "^3.0.0",
    "koa-proxy": "^0.6.0",
    "koa-static": "^2.0.0",
    "lodash": "4.16.6",
    "mobx": "^2.6.0",
    "mobx-react": "^3.5.8",
    "moment": "2.16.0",
    "nightwatch": "^0.9.12",
    "nightwatch-custom-commands-assertions": "^1.0.5",
    "node-sass": "^4.3.0",
    "react": "~15.3.0",
    "react-addons-test-utils": "~15.3.0",
    "react-autobind": "^1.0.6",
    "react-bootstrap": "^0.30.6",
    "react-bootstrap-date-picker": "^3.7.0",
    "react-dom": "~15.3.0",
    "react-dropzone": "3.7.3",
    "react-flickity-component": "0.1.0",
    "react-notification-system": "^0.2.11",
    "react-nouislider": "^1.14.2",
    "react-spinkit": "^1.1.11",
    "sass-loader": "^4.1.1",
    "satisfaction": "^3.0.0",
    "sinon": "^1.17.6",
    "sinon-as-promised": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-babel-jest": "^1.0.4",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-md5-hash": "^0.0.5",
    "xhr2": "^0.1.3"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-register": "^6.22.0",
    "bunyan": "^1.8.1",
    "cheerio": "^0.22.0",
    "co-mocha": "^1.1.3",
    "co-supertest": "^0.0.10",
    "eslint-loader": "^1.5.0",
    "hamjest": "^2.13.0",
    "mobx-react-devtools": "4.2.10",
    "mocha": "^3.2.0",
    "nodemon": "~1.11.0",
    "react-hot-loader": "^1.3.0",
    "stylefmt": "^5.1.2",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "supertest": "^2.0.1",
    "webpack-dev-server": "2.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/test/style-mock.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageDirectory": "<rootDir>/test/coverage",
    "coverageReporters": ["lcov", "text-summary"]
  }
}
