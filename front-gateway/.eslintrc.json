{
  "plugins": [
    "react",
    "jsx-a11y"
  ],
  "extends": [
    "plugin:react/recommended"
  ],
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "no-console": 0
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  }
}
