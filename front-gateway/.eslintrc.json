{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true,
    "jest": true,
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "plugins": [
      "jsx-a11y",
      "react"
    ],
    "extends": [
      "plugin:react/recommended",
      "./node_modules/dorbel-shared/eslint.shared.json"
    ]
  }
}
