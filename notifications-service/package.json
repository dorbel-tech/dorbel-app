{
  "name": "dorbel-notifications-service",
  "version": "0.0.5",
  "description": "Worker to process notifications messages from SQS and dispatch using Email or SMS",
  "main": "src/index.js",
  "scripts": {
    "start": "babel-node src/index",
    "start:dev": "npm run check; nodemon --exec babel-node src/index | bunyan",
    "test": "npm run test:unit",
    "test:unit": "mocha --compilers js:babel-register test/shared/before-tests.js test/unit/**/*.spec.js",
    "test:integration": "mocha --compilers js:babel-register test/shared/before-tests.js test/integration/**/*.spec.js",
    "lint": "eslint --ext .js -f table src",
    "check": "satisfaction-status || npm install"
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-register": "^6.16.3",
    "co": "^4.6.0",
    "co-mocha": "^1.1.3",
    "co-supertest": "0.0.10",
    "dorbel-shared": "git://github.com/dorbel-tech/dorbel-shared#v0.4.12",
    "eslint": "^3.7.1",
    "eslint-plugin-flowtype": "^2.19.0",
    "faker": "^3.1.0",
    "fleek-router": "^1.2.5",
    "hamjest": "^2.13.0",
    "koa": "^1.2.4",
    "koa-bodyparser": "^2.2.0",
    "lodash": "^4.16.3",
    "mandrill-api": "1.0.45",
    "mocha": "^3.1.0",
    "mock-require": "^1.3.0",
    "sinon": "^1.17.6",
    "sinon-as-promised": "^4.0.2",
    "supertest": "^1.2.0",
    "twilio": "2.11.0"
  },
  "devDependencies": {
    "bunyan": "^1.8.1",
    "flow-bin": "0.33.0",
    "nodemon": "^1.10.2",
    "satisfaction": "3.0.0"
  }
}
