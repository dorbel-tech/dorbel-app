{
  "AWSEBDockerrunVersion": 2,
  "authentication": { 
    "bucket": "dorbel-repository",
    "key": "dockercfg"
  },
  "volumes": [
    {
      "name": "apartments-api",
      "host": {
        "sourcePath": "/var/app/current/apartments-api"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "apartments-api",
      "image": "168720412882.dkr.ecr.eu-west-1.amazonaws.com/dorbel/apartments-api:latest",
      "essential": true,
      "memory": 128,
      "mountPoints": [
        {
          "sourceVolume": "apartments-api",
          "containerPath": "/var/www/app",
          "readOnly": true
        }
      ],
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 3000
        }
      ]
    }
  ]
}