{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "apartments-api",
      "host": {
        "sourcePath": "/var/app/current/apartments-api"
      }
    },
    {
      "name": "apartments-api-logs",
      "host": {
        "sourcePath": "/var/log/supervisor"
      }
    }    
  ],
  "containerDefinitions": [
    {
      "name": "apartments-api",
      "image": "168720412882.dkr.ecr.eu-west-1.amazonaws.com/dorbel/apartments-api:latest",
      "essential": true,
      "memory": 128,
      "mountPoints": [
        {
          "sourceVolume": "apartments-api",
          "containerPath": "/var/www/app",
          "readOnly": true
        },
        {
          "sourceVolume": "apartments-api-logs",
          "containerPath": "/var/log/containers/apartments-api",
          "readOnly": true
        }
      ],
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 3000
        }
      ]
    }
  ]
}
